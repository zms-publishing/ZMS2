<item type="dictionary"><dictionary>
<item key="custom"><![CDATA[<!-- -------// BO manage_main //------- -->

<dtml-call "REQUEST.set('data',getObjAttrValue(getObjAttr('text'),REQUEST))">

<dtml-if "REQUEST.get('btn','') in [ getZMILangStr('BTN_CHANGE'), getZMILangStr('BTN_UPLOAD')]">
 <dtml-if upload1>
  <dtml-if "upload1.filename[-3:] in ['jpg','gif','epg','png']">
    <dtml-call "REQUEST.set('newUpload',manage_addZMSGraphic(
      {'id_prefix':'source','active':1,'img':upload1},REQUEST))">
    <dtml-with newUpload>
     <dtml-call "REQUEST.set('data',data+'<div style='+QUOT+'margin-top:1em'+QUOT+'><img src='+QUOT+getImage(REQUEST).getHref(REQUEST)+QUOT+' /></div>')">
    </dtml-with>
  <dtml-else>
    <dtml-call "REQUEST.set('newUpload',manage_addZMSFile(
      {'id_prefix':'source','active':1,'file':upload1},REQUEST))">
    <dtml-with newUpload>
     <dtml-call "setObjProperty('title',getFile(REQUEST).getFilename(),lang)">
     <dtml-call "setObjProperty('titleshort',getFile(REQUEST).getFilename(),lang)">
     <dtml-call "REQUEST.set('data',data+'<div style='+QUOT+'margin-top:1em'+QUOT+'><a href='+QUOT+getFile(REQUEST).getHref(REQUEST)+QUOT+'>'+getTitlealt(REQUEST)+'</a></div>')">
    </dtml-with>
  </dtml-if>
  <div class="system-msg">
   <img src="<dtml-var MISC_ZMS>msg_info.gif" title="Info" border="0" align="absmiddle"/>
   <dtml-var "getZMILangStr('MSG_INSERTED')%('%s (%s)'%(upload1.filename,newUpload.meta_type))">
   (<dtml-var "getLangFmtDate(ZopeTime())">)
  </div>
 </dtml-if>
</dtml-if>

<script type="text/javascript">
<!--//
    function form0Submit(fm) {
      var b = true;
      try {
        if(typeof(document.getElementById('editor_text_<dtml-var lang>').EscapeUnicode) == 'undefined') {
          throw "Error"
        } else {
          document.getElementById('editor_text_<dtml-var lang>').EscapeUnicode = true;
          document.getElementById('xstandard_text_<dtml-var lang>').value = document.getElementById('editor_text_<dtml-var lang>').value;
        }			
      }
      catch(er) {
        document.getElementById('xstandard_text_<dtml-var lang>').value = document.getElementById('alternate_text_<dtml-var lang>').value;
      }
      <dtml-unless ZMS_INSERT>
      var upload1 = document.getElementById('upload1');
      if ( upload1 && !isBlank(upload1.value)) 
      {
        fm.action = 'manage_main';
      }
      </dtml-unless>
      return b;
    }

  var fm_form0 = document.forms['form0'];
  fm_form0.onsubmit = function() { return form0Submit(this) };

//-->
</script>

<!-- ### Text -->
<object id="editor_text_<dtml-var lang>" 
  <dtml-if "REQUEST.get('HTTP_USER_AGENT','').find('MSIE 7')>0">classid="clsid:0EED7206-1661-11D7-84A3-00606744831D"<dtml-else>type="application/x-xstandard"</dtml-if>
   width="100%" height="500"
   codebase="http://medvalue.net/pub/bscw.cgi/d293146/XStandard.cab#Version=2,0,0,0">
  <param name="CMSCode" value="9CF6B9BD-B73D-4206-8A77-8E05477E9726" />
  <param name="ToolbarWysiwyg" value="ordered-list, unordered-list, , draw-layout-table, draw-data-table, image, separator, hyperlink, attachment,  undo, wysiwyg, source, help" />
  <param name="EnablePasteMarkup" value="yes" />
  <param name="Styles" value="<dtml-var "getDocumentElement().absolute_url()">/f_xstandard_styles" />
  <param name="CSS" value="<dtml-var "getStylesheet().absolute_url()">" />
  <param name="Lang" value="<dtml-if "lang=='ger'">de<dtml-else>en</dtml-if>" />
  <param name="CMSImageLibraryURL" value="<dtml-var "getDocumentElement().absolute_url()">/f_xstandard_browseImages" />
  <param name="CMSAttachmentLibraryURL" value="<dtml-var "getDocumentElement().absolute_url()">/f_xstandard_browseFiles" />
  <param name="Options" value="1" />
  <param name="Value" value="<dtml-var data html_quote >" />
  <textarea name="alternate_text_<dtml-var lang>" id="alternate_text_<dtml-var lang>"><dtml-var data html_quote ></textarea>
</object>
<input type="hidden" name="text_<dtml-var lang>" id="xstandard_text_<dtml-var lang>" value="" />

<!-- ### Buttons -->
<dtml-var f_submitBtn>

<!-- ### Source -->
<dtml-unless ZMS_INSERT>
<input type="hidden" name="_id">
<input type="hidden" name="_sort_id:int" value="0">
<input type="hidden" name="custom" value="">
<table cellspacing="0" cellpadding="1" border="0" width="100%">
<colgroup>
 <col width="20%" />
 <col width="80%" />
</colgroup>
<dtml-let objAttr="getMetaobjAttr(meta_id,'source')" objLabel="objAttr['name']">
  <tr>
   <td colspan="2" class="zmiNeutralColorLight0"><div class="form-label"><dtml-var objLabel></div></td>
  </tr>
  <tr>
   <td class="zmiNeutralColorMedium0"><div class="form-label"><img src="<dtml-var MISC_ZMS>upload.gif" title="XML" border="0" align="absmiddle"><dtml-var "getZMILangStr('ATTR_FILE')"></div></td>
   <td>
    <input class="form-element" id="upload1" name="upload1" type="file" size="25">
    <input class="form-submit" name="btn" type="submit" value="<dtml-var "getZMILangStr('BTN_UPLOAD')">">
   </td>
  </tr>
  <dtml-in "getObjChildren(objAttr['id'],REQUEST)">
   <tr valign="top" id="tr_<dtml-var id>" class="zmiNeutralColorMedium0">
    <td style="border-top:1px solid white;border-bottom:1px solid #999999;" nowrap="nowrap"
     ><dtml-let actions="filtered_container_actions_objChildren(objAttr,_.str(id)+'/',REQUEST)"
      ><div class="form-small"
       ><dtml-if actions
        ><input class="form-checkbox" type="checkbox" name="ids:list" value="<dtml-var id>" onclick="switchButtons('zmiNeutralColorMedium0')"
       ></dtml-if
       ><a name="_<dtml-var id>" class="zmi" href="<dtml-var id>/manage_main?lang=<dtml-var lang>" onclick="javascript:return confirmChanges();"
        ><dtml-var f_display_icon
        ><dtml-if "objAttr.get('repetitive',0)==1"
         ><dtml-var "'%s %i'%(display_type(REQUEST),_['sequence-index']+1)"
        ><dtml-else
         ><dtml-var "display_type(REQUEST)"
        ></dtml-if
       ></a
      ></div
      ><dtml-if actions
       ><select class="form-small" onchange="submitSelect(this,'<dtml-var id>','<dtml-var "getSortId()">')"
        ><dtml-var "manage_main_actions(_,_,actions=actions,REQUEST=REQUEST)"
       ></select
      ></dtml-if
     ></dtml-let
    ></td>
    <td valign="top" <dtml-if "isActive(REQUEST)"><dtml-if "isPage()">class="zmiNeutralColorLight0"<dtml-else>class="zmiNeutralColorWhite"</dtml-if><dtml-else>class=" zmiNeutralColorLight1"</dtml-if> style="border-top:1px solid white;border-bottom:1px solid white;">
     <table cellspacing="0" cellpadding="0" border="0" width="100%">
     <tr valign="top">
      <td <dtml-if "isActive(REQUEST)"><dtml-if "isPage()">class="zmiNeutralColorLight0"<dtml-else>class="zmiNeutralColorWhite"</dtml-if><dtml-else>class=" zmiNeutralColorLight1"</dtml-if>>
       <div style="padding:3px"><dtml-var "renderShort(REQUEST)"></div>
      </td>
     </tr>
     </table>
    </td>
   </tr>
  </dtml-in>
 </dtml-let>
</table>
<dtml-var f_submitBtn>
</dtml-unless>

