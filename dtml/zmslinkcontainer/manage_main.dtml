<dtml-call "REQUEST.set( 'ZMS_INSERT', 'ZMSLinkElement')">

<table cellspacing="1" cellpadding="2" border="0" class="zmiNeutralColorStrong0">

<tr class="zmiNeutralColorMedium0">
 <td colspan="5"><dtml-var "f_headline(_,_,headline=getZMILangStr('BTN_ADD'))"></td>
</tr>

<tr valign="top" class="zmiNeutralColorStrong0">
 <td> &nbsp; </td>
 <td>
  <font class="form-label"><dtml-var "getZMILangStr('ATTR_URL')"></font>
  <font class="form-small">(<dtml-var "getZMILangStr('OR').lower()"> PMID)</font>
 </td>
 <td>
  <font class="form-label"><dtml-var "getZMILangStr('ATTR_TITLE')"></font>
  <font class="form-small">(<dtml-var "getZMILangStr('OR').lower()"> &quot;Medline&quot;)</font>
 </td>
 <td colspan="2">
  <font class="form-label"><dtml-var "getZMILangStr('ATTR_DESCRIPTION')"></font>
  <font class="form-small">(<dtml-var "getZMILangStr('LITERATURE_REFERENCE_ONLY')">!)</font>
 </td>
</tr>

<tr valign="top" class="zmiNeutralColorLight0">
 <td class="zmiNeutralColorMedium0"><img src="<dtml-var spacer_gif>" title="" border="0" width="40" height="20"></td>
 <td class="form-small"><dtml-var "getUrlInput(fmName='form0',elName='_url',elTextName='_title',size=15,value='',enabled=1,REQUEST=REQUEST,css='form-small')">&nbsp;</td>
 <td class="form-small"><dtml-var "getTextInput(fmName='form0',elName='_title',size=10,value='',enabled=1,REQUEST=REQUEST,css='form-small')"></td>
 <td class="form-small"><dtml-var "getTextArea(fmName='form0',elName='_description',cols=15,rows=3,value='',enabled=1,REQUEST=REQUEST,css='form-small')"></td>
 <td width="100">&nbsp;</td>
</tr>

</table>

<!-- ### SUBMIT -->
