 <td><dtml-var "getObjInput('titleshort',REQUEST)"></td>
</tr>
 
<!-- ### Title -->
<tr valign="middle">
 <td><div class="form-label"><dtml-var "getLangStr('ATTR_TITLE',manage_lang)"> <sup style="color:red">*</sup></div></td>
 <td><dtml-var "getObjInput('title',REQUEST)"></td>
