<dtml-if "isCheckedOut(REQUEST) and 
          _.hasattr(this(),'co_uid_%s'%lang) and
          _.getattr(this(),'co_uid_%s'%lang,_.None) != _.str(AUTHENTICATED_USER) and
          not (REQUEST.has_key('btn') and REQUEST['btn'] == getLangStr('BTN_OVERTAKE',manage_lang))">
  <dtml-call "RESPONSE.redirect('checkout_overtake_html?lang=%s&manage_lang=%s'%(lang,manage_lang))">
<dtml-else>
  <dtml-call "this().checkOut(REQUEST)">
</dtml-if>
